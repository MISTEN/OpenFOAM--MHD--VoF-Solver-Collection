FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      JH;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -3 0 0 0 0]; // W/mÂ³

internalField   uniform 0;

boundaryField
{
    ".*"
    {
        type            zeroGradient;
		value 			$internalField;
    }
}

